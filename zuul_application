spring:
  profiles:
    active: dev


---
server:
  port: 9527
spring:
  application:
    name: MICROSERVICE-ZUUL-GATEWAY
  profiles: dev
eureka:
  client:
    service-url:
      defaultZone: http://localhost:7001/eureka
  instance:
    instance-id: xinye-9527
    prefer-ip-address: true # 是否显示ip
zuul:
  prefix: "/aigou"
  ignored-services: "*"
  routes:
    plat.serviceId: plat-service
    plat.path: /plat/**

---
server:
  port: 9527
spring:
  application:
    name: MICROSERVICE-ZUUL-GATEWAY
  profiles: test
eureka:
  client:
    service-url:
      defaultZone: http://localhost:7001/eureka
  instance:
    instance-id: xinye-9527
    prefer-ip-address: true # 是否显示ip
zuul:
  prefix: "/aigou"
  ignored-services: "*"
  routes:
    plat.serviceId: plat-service-test
    plat.path: /plat/**
